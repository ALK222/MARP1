{
  "spellright.language": ["es", "en"],
  "spellright.documentTypes": ["markdown", "plaintext", "tex", "latex"],
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex, makeglossaries, xelatex (2x)",
      "tools": [
        "xelatex",
        //"makeglossaries",
        "xelatex",
        "xelatex"
      ]
    },
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    },
    {
      "name": "xelatex, makeglossaries, bibtex, xelatex (2x)",
      "tools": [
        "xelatex",
        //"makeglossaries",
        //"bibtex",
        "xelatex",
        "xelatex"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": ["-file-line-error", "-interaction=nonstopmode", "%DOC%"],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": ["-f", "%DOC%"]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"],
      "env": {}
    },
    {
      "name": "makeglossaries",
      "command": "makeglossaries",
      "args": ["%DOCFILE%"]
    }
  ],
